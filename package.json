{
  "name": "phonenumbergenerator",
  "version": "1.0.0",
  "description": "PhoneNumberGenerator generates random phone numbers for a telecommunications company",
  "main": "index.js",
  "scripts": {
    "start-dev": "nodemon --exec babel-node src/server/index.js",
    "start": "node dist/server.js",
    "build": "babel src -d dist -s",
    "test": "NODE_ENV=test nyc --reporter=text --reporter=lcov mocha --require @babel/register tests/server/index.spec.js --exit",
    "verse": " babel --version",
    "coveralls:report": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "exclude": [
      "**/**/database",
      "tests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Felglitzzz/PhoneNumberGenerator.git"
  },
  "author": "Felix Amande",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Felglitzzz/PhoneNumberGenerator/issues"
  },
  "homepage": "https://github.com/Felglitzzz/PhoneNumberGenerator#readme",
  "dependencies": {
    "@types/express": "^4.16.1",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "jest": "^24.0.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "sinon": "^7.2.3",
    "supertest": "^3.4.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1"
  }
}
