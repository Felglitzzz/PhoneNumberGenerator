{
  "name": "phonenumbergenerator",
  "version": "1.0.0",
  "description": "PhoneNumberGenerator generates random phone numbers for a telecommunications company",
  "main": "index.js",
  "scripts": {
    "start-server-dev": "nodemon --exec babel-node src/dist-server/index.js",
    "start": "NODE_ENV=production node src/dist-server/index.js",
    "start:dev": "npm run clean && npm run build-dev && npm run start-server-dev",
    "build": "babel src -d dist -s",
    "test": "NODE_ENV=test nyc --reporter=text --reporter=lcov mocha --require @babel/register tests/server/index.spec.js --exit",
    "verse": " babel --version",
    "coveralls:report": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "lint-staged",
    "format:fix": "pretty-quick --staged && eslint src/ --fix",
    "clean": "rimraf src/dist-server && rimraf src/dist-client",
    "build-server": "babel -d src/dist-server --copy-files src/server -s",
    "build-client-dev": "webpack --config src/client/webpack.dev.js",
    "build-client-prod": "webpack -p --config src/client/webpack.prod.js",
    "build-dev": "npm run build-server && npm run build-client-dev",
    "heroku-postbuild": "npm run build-server && npm run build-client-prod"
  },
  "nyc": {
    "exclude": [
      "**/**/database",
      "tests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s -l format:fix"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Felglitzzz/PhoneNumberGenerator.git"
  },
  "author": "Felix Amande",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Felglitzzz/PhoneNumberGenerator/issues"
  },
  "homepage": "https://github.com/Felglitzzz/PhoneNumberGenerator#readme",
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@types/express": "^4.16.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.17.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.4",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "sinon": "^7.2.3",
    "supertest": "^3.4.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "webpack-dev-server": "^3.2.0"
  }
}
